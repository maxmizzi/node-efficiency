{
  "rules": {
    "lazy-import-heavy-js-deps": {
      "enabled": true,
      "severity": "warning",
      "additionalHeavyDeps": []
    }
  },
  "patterns": [
    "lib/**/*.js",
    "lib/**/*.mjs",
    "benchmark/**/*.js",
    "benchmark/**/*.mjs"
  ],
  "exclude": [
    "**/node_modules/**",
    "**/test/**",
    "**/*.test.js",
    "**/*.spec.js"
  ]
}